{
  "name": "idtf-v3.5",
  "version": "3.5.0",
  "description": "Industrial Digital Twin Framework - A comprehensive framework for industrial asset digital twin management",
  "private": true,
  "workspaces": [
    "packages/*",
    "examples/*"
  ],
  "scripts": {
    "lint": "eslint . --ext .js,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.ts,.tsx --fix",
    "schema:validate": "node scripts/validate-schemas.js",
    "test": "jest",
    "test:e2e": "node scripts/e2e-tests.js",
    "build": "npm run build --workspaces",
    "clean": "npm run clean --workspaces && rm -rf node_modules"
  },
  "devDependencies": {
    "eslint": "^8.50.0",
    "jest": "^29.7.0",
    "ajv": "^8.12.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chchlin1018/IDTF-V3.5.git"
  },
  "license": "Apache-2.0",
  "author": "IDTF Contributors"
}

